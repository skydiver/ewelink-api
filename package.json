  "dependencies": {
    "arpping": "github:skydiver/arpping",
    "crypto-js": "^4.1.1",
    "delay": "^5.0.0",
    "node-fetch": "^2.6.7",
    "random": "^3.0.6",
    "websocket": "^1.0.34",
    "websocket-as-promised": "^2.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-wesbos": "3.0.2",
    "eslint-plugin-html": "^6.1.0",
    "eslint-plugin-import": "^2.26.0",
