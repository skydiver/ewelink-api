  "license": "MIT",
  "typings": "./index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skydiver/ewelink-api.git"
  },
  "bugs": {
    "url": "https://github.com/skydiver/ewelink-api/issues"
  },
  "keywords": [
    "ewelink",
    "sonoff",
    "api",
    "node",
    "nodejs"
  ],
  "main": "main.js",
  "scripts": {
